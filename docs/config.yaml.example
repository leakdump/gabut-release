# Gabut Configuration File

# General Settings
timeout: 10           # Connection timeout in seconds
threads: 50           # Number of concurrent threads
user_agent: "Gabut/1.0"

# Port Configuration
# Combined list of all ports to scan
ports:
  - 21    # FTP
  - 22    # SSH
  - 23    # Telnet
  - 25    # SMTP
  - 53    # DNS
  - 80    # HTTP
  - 110   # POP3
  - 135   # RPC
  - 139   # NetBIOS
  - 143   # IMAP
  - 443   # HTTPS
  - 993   # IMAPS
  - 995   # POP3S
  - 1723  # PPTP
  - 3306  # MySQL
  - 3389  # RDP
  - 5432  # PostgreSQL
  - 5900  # VNC
  - 8080  # HTTP-Alt
  - 8443  # HTTPS-Alt
  - 8000  # HTTP-Alt
  - 8888  # HTTP-Alt
  - 9000  # HTTP-Alt
  - 9090  # HTTP-Alt

# DNS Resolvers
resolvers:
  - "8.8.8.8"
  - "1.1.1.1"
  - "9.9.9.9"
  - "208.67.222.222"

# CertStream Configuration
certstream:
  enabled: true
  url: "wss://ct.debian.so"
  auto_scan: true

  # Keywords to filter domains (empty = monitor all)
  keywords:
    - "api"
    - "dev"
    - "staging"
    - "test"
    - "admin"
    - "internal"
    - "coin"
    - "finance"
    - "token"
    - "crypto"
    - "trading"
    - "blockchain"
    - "wallet"
    - "exchange"
    - "payment"
    - "transaction"

  # Whitelist domains (empty = allow all)
  whitelist: []

  # Blacklist domains to skip
